public class Aula0052A {

    public static void main(String[] args) {

        int[] nums = new int[28];
        int media = 0;
        for(int i = 0; i<nums.length; i++)
            media += nums[i];

        float total = (float)media / nums.length;

        System.out.println("A média é: "+total);
    }

}
